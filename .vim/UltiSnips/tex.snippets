snippet b "begin{} ... end{}"
\begin{${1:section}}
	${VISUAL}$0
\end{$1}
endsnippet

snippet i "Item"
\item
endsnippet

snippet em "Emphasis"
\emph{$1}$0
endsnippet

snippet em "Typewriter"
\texttt{$1}$0
endsnippet

snippet m "Inline math"
\($1\)$0
endsnippet

snippet # "section"
\section{${1:name}}
	${VISUAL}$0
endsnippet

snippet ## "subsection"
\subsection{${1:name}}
	${VISUAL}$0
endsnippet

snippet ### "subsubsection"
\subsubsection{${1:name}}
	${VISUAL}$0
endsnippet
